<div class="main">
	<nav class="main_hero-nav">
		<h2>Dashboard</h2>
		<div class="main_hero-navContent">
			<input class="main_hero-navContent-inputSearch" type="text" placeholder="Search" id="input_search">
			<span class="material-symbols-outlined">
				home
			</span>
			<span class="material-symbols-outlined">
